# Backend Environment Variables Template
# Copy this file to .env.development.local and fill in your actual values

# Server Configuration
PORT=3090

# MongoDB Connection String
DB_URI=mongodb+srv://username:password@cluster.mongodb.net/?appName=YourApp

# API Key for admin-protected routes (must match frontend VITE_API_KEY)
API_KEY=your-api-key-here

# JWT Secret for token signing
JWT_SECRET=your-jwt-secret-here

